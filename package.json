{
  "name": "hedera-agents-hub",
  "version": "0.1.0",
  "description": "Agent-first Digital Economy using ERC-8004 and x402 on Hedera testnet",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node",
    "test": "node --test dist/tests/**/*.test.js",
    "test:watch": "ts-node --watch",
    "clean": "rm -rf dist",
    "start": "node dist/server/index.js",
    "kill:8080": "lsof -ti:8080 | xargs kill -9",
    "restart": "npm run kill:8080 && npm run build && npm run start",
    "deploy:env": "node deploy-env.js",
    "deploy": "npm run build && railway up",
    "migrate": "node run-migration.js",
    "test:db": "node test-db-connection.js"
  },
  "keywords": [
    "hedera",
    "erc-8004",
    "x402",
    "agents",
    "blockchain",
    "payments"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hashgraph/sdk": "^2.64.5",
    "@supabase/supabase-js": "^2.78.0",
    "@types/jsonwebtoken": "^9.0.10",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.10.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.15.6",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
